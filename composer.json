{
    "name": "elao/form-theme-bundle",
    "type": "symfony-bundle",
    "description": "Generate theme key for forms",
    "keywords": ["elao", "form", "theme"],
    "homepage": "https://github.com/Elao/ElaoFormThemeBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Elao",
            "homepage": "http://www.elao.com"
        },
        {
            "name": "Maxime Colin",
            "homepage": "https://github.com/maximecolin"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.8|~3.0",
        "symfony/form": "~2.8|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4"
    },
    "autoload": {
        "psr-4": {
            "Elao\\Bundle\\FormThemeBundle\\": "."
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
